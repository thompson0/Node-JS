{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "double"],
      "semi": ["error", "never"],
      "linebreak-style": ["error", "windows"]
    }
